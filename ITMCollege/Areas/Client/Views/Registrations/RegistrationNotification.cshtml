@{
    ViewBag.Title = "Registration Notification";
}
@{
    Layout = "~/Views/Shared/Client/_Layout.cshtml";
}
<div class="site-section ftco-subscribe-1 site-blocks-cover pb-4" style="background-image: src(~/Images/bg_1.jpg)">
    <div class="container">
        <div class="row align-items-end">
            <div class="col-lg-7">
                <h2 class="mb-0">REGISTRATION</h2>
            </div>
        </div>
    </div>
</div>
<div class="custom-breadcrumns border-bottom">
    <div class="container">
        <a asp-area="Client" asp-controller="Home" asp-action="Index">Home</a>
        <span class="mx-3 icon-keyboard_arrow_right"></span>
        <span class="current">Registration</span>
    </div>
</div>
<div></div>
<div class="container">
    @if (ViewBag.RegStatus == 0)
    {
        <div class="alert-primary my-5">
            <p class="px-2 font-weight-bold font-italic py-3">Congratulations, Registration with Registration Number <span class="text-danger font-weight-bold">@ViewBag.RegNum</span> is successfull.</p>
        </div>
    }
    else
    {
        <div class="alert-danger my-5">
            <p class="px-2 font-weight-bold font-italic py-3">Fail resgistration . Please try again later.</p>
        </div>
    }


</div>
